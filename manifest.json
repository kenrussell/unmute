{
    "name": "Unmute",
    "description": "Provides UI for new autoplay policy",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "tabs",
        "notifications"
    ],
    "browser_action": {
    },
    "background": {
        "scripts": ["background.js"]
    },
    "manifest_version": 2
}
